---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0"    xmlns:itunes="http://www.itunes.com/dtds/podcast-1.0.dtd"  xmlns:content="http://purl.org/rss/1.0/modules/content/">
  <channel>
    <atom:link href="https://blog.josh.me.uk/slletshow/feed.xml/" rel="self" type="application/rss+xml" />
    <title>{{ site.slletshow.title }}</title>
    <link>{{ site.slletshow.link }}</link>
    <itunes:author>{{ site.slletshow.author }}</itunes:author>
    <copyright>{{ site.license }}</copyright>
    <language>en-GB</language>
    <pubDate>{{ site.time | date_to_rfc822 }}</pubDate>
    <lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
    <itunes:category text="{{ site.slletshow.category }}">
      {% for subcategory in site.slletshow.subcategories %}  <itunes:category text="{{ subcategory }}"/>{% endfor %}
    </itunes:category>
    <itunes:explicit>{{ site.slletshow.explicit }}</itunes:explicit>
    <itunes:image href="{{ site.slletshow.art }}"/>
    <itunes:subtitle>{{ site.slletshow.subtitle }}</itunes:subtitle>
    <description>{{ site.slletshow.summary }}</description>
    <itunes:summary>{{ site.slletshow.summary }}</itunes:summary>
    <itunes:owner>
      <itunes:name>{{ site.slletshow.author }}</itunes:name>
      <itunes:email>{{ site.slletshow.email }}</itunes:email>
    </itunes:owner>
    <generator>Jekyll v{{ jekyll.version }}</generator>
    {% for post in site.categories.slletshow %}<item>
      <title>{{ post.episode }}:"{{ post.title }}"</title>
      <description>{{ post.description | strip_html }}</description>
      <pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
      <enclosure
        url="{{ post.mp3 }}"
        length="{{ post.duration | round }}"
        type="audio/mp3"/>
      <link>{{ site.url }}{{ post.url }}</link>
      <guid>{{ site.url }}{{ post.url }}</guid>
      <itunes:title>{{ post.title }}</itunes:title>
      {% unless post.episode == Null %}<itunes:episode>{{ post.episode }}</itunes:episode>{% endunless %}
      {% unless post.season == Null %}<itunes:season>{{ post.season }}</itunes:season>{% endunless %}
      {% unless post.type == Null %}<itunes:episodeType>{{ post.type }}</itunes:episodeType>{% endunless %}
      <itunes:author>{{ site.slletshow.author }}</itunes:author>
      <itunes:subtitle>{{ post.description | strip_html }}</itunes:subtitle>
      <itunes:summary>{{ post.description | strip_html }}</itunes:summary>
      <itunes:explicit>{{ post.explicit }}</itunes:explicit>
      <itunes:duration>{{ post.duration }}</itunes:duration>
      <content:encoded>
        <![CDATA[{{post.content}}]]>
      </content:encoded>
      <itunes:image href="{{ post.artwork }}"/>
      </item>{% endfor %}
  </channel>
</rss>
